{
  "name": "vue-use-route-query",
  "version": "1.2.0",
  "author": "Kirill Romanov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Djaler/vue-use-route-query"
  },
  "keywords": [
    "vue",
    "router",
    "vue-router",
    "query",
    "composition-api",
    "vue-composition-api"
  ],
  "scripts": {
    "switch-vue2": "vue-demi-switch 2 vue2",
    "switch-vue2.7": "vue-demi-switch 2.7 vue2.7",
    "switch-vue3": "vue-demi-switch 3",
    "test": "vitest run",
    "check-es6": "es-check es2017 dist/index.js",
    "check-treeshake": "agadoo dist/index.mjs",
    "build:types": "tsc --project tsconfig.build.json",
    "build": "vite build && npm run build:types",
    "postbuild": "npm run check-es6 && npm run check-treeshake",
    "publish": "clean-publish"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "async-queue-chain": "^1.1.0",
    "vue-demi": "^0.13.1"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.1.0",
    "vue": "^2.6.10 || ^3.2.0",
    "vue-router": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "agadoo": "2.0.0",
    "clean-publish": "4.1.1",
    "es-check": "7.1.0",
    "vite": "2.9.16",
    "vitest": "0.22.1",
    "vue": "3.2.33",
    "vue-router": "4.0.14",
    "vue2": "npm:vue@2.6.14",
    "vue2.7": "npm:vue@2.7.0"
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "publishDirectory"
  }
}
